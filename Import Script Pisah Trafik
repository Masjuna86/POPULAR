# Speedtest
/tool fetch url="https://raw.githubusercontent.com/Masjuna86/Pisah-Trafik/refs/heads/main/Speedtest.rsc" dst-path="Speedtest.rsc"
/import file-name="Speedtest.rsc"
/file remove "Speedtest.rsc"
/routing table add name="To-Speedtest" fib comment="Route by Juna"
/ip route add check-gateway=ping distance=1 gateway="192.168.100.1" routing-table="To-Speedtest" comment="Route by Juna"
/ip firewall mangle add action=mark-routing chain=prerouting src-address-list=LOCAL-IP dst-address-list="Speedtest" new-routing-mark="To-Speedtest" comment="Speedtest" passthrough=no place-before=*0

# Streaming
